<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
          assembly="SynergyGestion.Dominio"
          namespace="SynergyGestion.Dominio.Modelo.Compras" default-access="field">
  
  <class name="Proveedor" table="Proveedor" schema="Compras">
    <id name="id">
      <generator class="hilo">
        <param name="table">hibernate_unique_key</param>
        <param name="column">next_hi</param>
      </generator>
    </id>
    <property name="nombre" not-null="true" />
    <property name="cuit" not-null="true" />
  </class>
</hibernate-mapping>